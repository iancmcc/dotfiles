!-- Xft settings -- !
Xft.dpi:        227
Xft.rgba:       rgb
Xft.autohint:   1
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    0
Xft.antialias:  1

 ! -- Fonts -- !
! URxvt.font:xft:DejaVuSansMono:size=11,style=Regular, xft:Symbola:size=11,style=Regular
! URxvt.boldFont:xft:DejaVuSansMono:style=Bold,size=11,minspace=False xft:Symbola:size=11,style=Regular
URxvt.font:xft:InputMonoNarrow Nerd Font:size=11
URxvt.boldFont:xft:InputMonoNarrow Nerd Font:size=11,style=Bold
URxvt.letterSpace: -2

*faceSize:              10
*bellIsUrgent:          true

!! scroll
!! do not scroll with output
URxvt*scrollTtyOutput:      false
!! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer:     true
!! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress:    true

!! scrollback buffer
URxvt.secondaryScreen:  1
URxvt.secondaryScroll:  0
URxvt.secondaryWheel:   1

!! scrollbar
URxvt.scrollBar:             false

URxvt.termName:         rxvt-256color

URxvt.dpi: 227

!! copy paste
URxvt*clipboard.copycmd:    xclip -i -selection clipboard
URxvt*clipboard.pastecmd:   xclip -o -selection clipboard

!! clickable urls
URxvt.perl-ext-common:      default,matcher,selection-to-clipboard
URxvt.url-launcher:         /usr/local/bin/browser-exec
URxvt.matcher.button:       1
URxvt.url-select.underline: true

!! get option over to work
URxvt.keysym.M-Left:        \033[1;5D
URxvt.keysym.M-Right:       \033[1;5C
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C



!------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.fullscreen: false
rofi.combi-modi: window,drun,windowcd
rofi.fuzzy:  true
rofi.opacity: 70
rofi.color-enabled: true
! Enable the extended coloring options

!                  bg       border   separator
rofi.color-window: #32302f, #a89984, #a89984

!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #32302f, #ebdbb2, #3c3836, #665c54, #fbf1c7
rofi.color-active: #d79921, #32302f, #d79921, #fabd2f, #32302f
rofi.color-urgent: #cc241d, #32302f, #cc241d, #fb4934, #32302f



! rofi.font: System San Francisco Display 20
rofi.font: InputSansNarrow 20,FontAwesome 20
! rofi.fg: argb:00ffffff
rofi.eh: 1
rofi.line-margin: 10
rofi.separator-style: solid
rofi.hide-scrollbar: true
rofi.lines: 20
rofi.width: 50
rofi.padding: 50
rofi.bw: 0
rofi.auto-select: false
rofi.scroll-method: 1
rofi.window-format: {w:5} {t}
rofi.mesg: "sup"
rofi.show-icons: true


URxvt.transparent:   true
URxvt.shading:       25
